			<Text  id="txt_disable" state="disable|ckdisable"  pos="8|5,0,5,0"  halign="center" valign="center" endellipsis="1" textcolor="79848D"/>
		</Style>

		<Style id="TabCtrl">
			<Image id="bk" state="non" pos="8|0,0,0,0"/>
		</Style>

		<Style id="TabHeader">
			<Image id="bk" state="non" pos="8|0,0,0,0" image="#tabbkg"/>
		</Style>

