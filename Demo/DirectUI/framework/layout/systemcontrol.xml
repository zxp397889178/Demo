			<Image id="arrow" state="non"      pos="9|11,-1,-11,11" image="#arrow"/>
		</Style>

		<Style id="TabButton">
			<Image id="bk" state="all|cknormal|ckover|ckdown|ckfocus"  image="#tabbutton"  imgseq="0,1,2,-1,-1" pos="8|0,0,0,0" />
